{
  "name": "printwatch-a1-card",
  "version": "1.2.0",
  "description": "A Home Assistant card for A1 series 3D printers",
  "type": "module",
  "main": "dist/printwatch-a1-card.js",
  "scripts": {
    "build": "rollup -c",
    "watch": "rollup -c --watch",
    "lint": "eslint src/*",
    "clean": "rimraf dist",
    "version:patch": "node bump-version.js patch",
    "version:minor": "node bump-version.js minor",
    "version:major": "node bump-version.js major",
    "release": "npm run build && git add dist/ && git commit -m 'Update build' && git push"
  },
  "keywords": [
    "home-assistant",
    "homeassistant",
    "hacs",
    "bambu-lab",
    "3d-printer"
  ],
  "author": "Steven Hubert",
  "license": "MIT",
  "dependencies": {
    "@lit/reactive-element": "^2.0.0",
    "lit": "^2.8.0",
    "lit-html": "^3.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0 || ^8.0.0",
    "@babel/preset-env": "^7.20.0 || ^8.0.0",
    "@rollup/plugin-babel": "^6.0.0",
    "@rollup/plugin-commonjs": "^24.0.0 || ^25.0.0 || ^26.0.0",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0 || ^16.0.0",
    "@rollup/plugin-replace": "^5.0.0",
    "@rollup/plugin-terser": "^0.4.0",
    "eslint": "^8.0.0",
    "rimraf": "^5.0.0 || ^6.0.0",
    "rollup": "^3.0.0 || ^4.0.0"
  }
}
